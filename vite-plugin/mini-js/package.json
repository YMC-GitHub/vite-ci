{
  "name": "@yors/vite-plugin-mini-js",
  "version": "1.0.1",
  "description": "a library package , as vite plugin , to compress js file for bunlding",
  "keywords": [
    "@yors",
    "vite-plugin",
    "mini-js"
  ],
  "author": "yemiancheng <ymc.github@gmail.com> (https://github.com/ymc-github)",
  "license": "MIT",
  "private": false,
  "main": "./dist/main.cjs",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.mjs",
      "require": "./dist/main.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "types": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymc-github/vite-ci.git"
  },
  "bugs": {
    "url": "https://github.com/ymc-github/vite-ci/issues"
  },
  "homepage": "https://github.com/ymc-github/vite-ci/blob/main/vite-plugin/mini-js#readme",
  "devDependencies": {
    "@types/node": "^20.11.30",
    "rollup": "^4.13.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.6"
  },
  "dependencies": {
    "terser": "^5.29.2"
  }
}
